{
    "name": "cxxswizzle",
    "version-string": "2.0.0",
    "dependencies": [
        {
            "name": "sdl2-image",
            "features": [ "libjpeg-turbo" ]
        },
        {
            "name": "imgui",
            "features": [ "sdl2-binding", "sdl2-renderer-binding" ]
        },
        {
            "name": "sdl2",
            "features": ["x11"],
            "platform": "linux&!android",
            "$reason": "https://github.com/microsoft/vcpkg/issues/21750"
        },
        {
            "name": "sdl2",
            "platform": "!(linux&!android)"
        },
        "gtest",
        {
            "name": "vc",
            "platform": "!(arm64&osx)"
        }
    ]
}
